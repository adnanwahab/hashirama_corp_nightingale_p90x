{
  "expo": {
    "name": "HashiramaCorp_RobotDoctor",
    "slug": "HashiramaCorp_RobotDoctor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ATV-CTRL"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ee558623-1361-4efb-bd74-657684da3de8"
      }
    },
    "plugins": [
      "expo-build-properties", {
        "ios" : {"newArchEnabled": true},
        "android" : {"newArchEnabled": true}
      }
    ]
  }
}
